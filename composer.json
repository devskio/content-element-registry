{
	"name": "digitalwerk/content-element-registry",
	"type": "typo3-cms-extension",
	"description": "Content element registry helper for registering Typo3 content elements",
	"version": "11.5.3",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": ">=11 <12"
	},
	"autoload": {
		"psr-4": {
			"Digitalwerk\\ContentElementRegistry\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "content_element_registry"
		}
	},
	"replace": {
		"digitalwerk/content_element_registry": "self.version",
		"typo3-ter/content-element-registry": "self.version"
	},
	"authors": [
		{
			"name": "Ondrej Grosko",
			"email": "ondrej@digitalwerk.agency",
			"homepage": "https://digitalwerk.agency/"
		}
	]
}
