{
	"name": "devskio/content-element-registry",
	"type": "typo3-cms-extension",
	"description": "Content element registry helper for registering Typo3 content elements",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": ">=11 <12"
	},
	"autoload": {
		"psr-4": {
			"Devsk\\ContentElementRegistry\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "content_element_registry"
		}
	},
	"replace": {
		"devsk/content_element_registry": "self.version",
		"typo3-ter/content-element-registry": "self.version"
	},
	"authors": [
		{
      "name": "DEVSK Team",
      "email": "development@devsk.io",
      "homepage": "https://www.devsk.io/"
		}
	],
  "conflict": {
    "digitalwerk/content-element-registry": "*"
  }
}
